{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "jsx": "react",
    "lib": ["esnext", "dom"],
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "alwaysStrict": false,
    "removeComments": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": ["../../typings"]
}
