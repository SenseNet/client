{
  "name": "sn-react-redux-todo-app",
  "version": "2.0.5",
  "description": "Todo app with React and Redux built upon Sense/Net ECM",
  "main": "index.tsx",
  "private": true,
  "scripts": {
    "precommit": "npm run test",
    "commit": "git-cz",
    "pretest": "rimraf temp && tsc -p tsconfig.test.json",
    "test:jest": "jest",
    "fix": "npm run fix:prettier && npm run fix:tslint",
    "fix:prettier": "prettier \"{,!(dist|temp|bundle)/**/}*.{ts,tsx}\" --write",
    "fix:tslint": "tslint --fix --project .",
    "lint": "tslint --project .",
    "build:webpack": "rimraf bundle && webpack",
    "publish:development": "npm run build && npm t && npm run typedoc && npm publish --tag development",
    "prepublishOnly": "npm run build",
    "typedoc": "typedoc --mode file --out documentation src --tsconfig tsconfig.json --theme c:/Users/%USERNAME%/AppData/Roaming/npm/node_modules/@sensenet/typedoc-theme/sn-theme",
    "start": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SenseNet/sn-react-redux-todo-app.git"
  },
  "keywords": [
    "sensenet",
    "react",
    "redux"
  ],
  "author": "Sense/Net <snteam@sensenet.com> (http://sensenet.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SenseNet/sn-react-redux-todo-app.git/issues"
  },
  "homepage": "https://sensenet.com",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "@sensenet/authentication-google": "^2.0.6",
    "@sensenet/client-core": "^1.5.1",
    "@sensenet/controls-react": "^2.5.1",
    "@sensenet/redux": "^5.1.7",
    "material-ui-pickers": "^2.0.4",
    "radium": "^0.25.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-quill": "^1.3.3",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "redux-di-middleware": "3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.9",
    "@types/react": "^16.7.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux-logger": "^3.0.6",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^5.0.0",
    "jest": "^24.1.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.0.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.8"
  }
}
