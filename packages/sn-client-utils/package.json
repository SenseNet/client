{
  "name": "@sensenet/client-utils",
  "version": "1.4.2",
  "description": "General sensenet ECM independent client side utilities",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src",
    "test"
  ],
  "scripts": {
    "precommit": "npm run test",
    "commit": "git-cz",
    "test:jest": "jest",
    "fix": "npm run fix:prettier && npm run fix:tslint",
    "fix:prettier": "prettier \"{,!(dist|temp)/**/}*.{ts,tsx}\" --write",
    "fix:tslint": "tslint --fix --project .",
    "lint": "tslint --project .",
    "build": "tsc -b",
    "prepublishOnly": "npm run build",
    "publish:development": "npm run build && npm t && npm run typedoc && npm publish --tag development",
    "typedoc": "typedoc --mode file --out documentation src --tsconfig tsconfig.json --theme c:/Users/%USERNAME%/AppData/Roaming/npm/node_modules/@sensenet/typedoc-theme/sn-theme"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SenseNet/sn-client.git"
  },
  "keywords": [
    "sensenet",
    "ecm",
    "javascript",
    "REST",
    "ajax",
    "API"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Sense/Net <snteam@sensenet.com> (http://sensenet.com/)",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/SenseNet/sn-client/issues"
  },
  "homepage": "https://sensenet.com",
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5"
  },
  "config": {
    "commitizen": {
      "path": "sensenet-kfi-cz-conventional-changelog"
    }
  },
  "typings": "./dist/index.d.ts"
}
