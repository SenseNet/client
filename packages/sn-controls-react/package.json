{
  "name": "@sensenet/controls-react",
  "version": "8.0.0",
  "description": "React controls for sensenet",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "typings": "./dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "fix:prettier": "prettier \"{,!(dist|temp)/**/}*.{ts,tsx}\" --write",
    "dev": "npm-run-all -p \"build:types -w\" \"build:esm -w\"",
    "build:release": "npm-run-all -p build:esm build:cjs build:bundles",
    "build:esm": "cross-env BABEL_ENV=esm babel src --extensions '.ts,.tsx' --out-dir 'dist/esm' --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts,.tsx' --out-dir 'dist/cjs' --source-maps",
    "build:bundles": "cross-env BABEL_ENV=bundle rollup -c",
    "build:types": "tsc -p tsconfig.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SenseNet/sn-client.git"
  },
  "keywords": [
    "sensenet",
    "cms",
    "csp",
    "react",
    "reactjs",
    "controls",
    "components"
  ],
  "author": "Sense/Net <snteam@sensenet.com> (http://sensenet.com/)",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/SenseNet/sn-client/issues"
  },
  "homepage": "https://sensenet.com",
  "peerDependencies": {
    "@material-ui/core": "^4.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@date-io/core": "^1.3.13",
    "@date-io/date-fns": "1.3.13",
    "@material-ui/pickers": "^3.3.10",
    "@sensenet/client-core": "^4.0.0",
    "@sensenet/client-utils": "^2.1.1",
    "@sensenet/control-mapper": "^3.1.1",
    "@sensenet/default-content-types": "^5.0.0",
    "@sensenet/editor-react": "^0.1.0",
    "@sensenet/hooks-react": "^1.6.1",
    "@sensenet/pickers-react": "^2.1.1",
    "@sensenet/query": "^2.1.1",
    "@sensenet/search-react": "^2.1.2",
    "date-fns": "^2.21.3",
    "react-color": "^2.18.1",
    "react-responsive": "^8.2.0",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@material-ui/core": "^4.11.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/enzyme": "^3.10.7",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^17.0.5",
    "@types/react-responsive": "^8.0.2",
    "babel-plugin-add-import-extension": "^1.5.1",
    "cross-env": "^7.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "jest": "^26.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.15",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.48.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
